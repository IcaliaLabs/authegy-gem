# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Add a newline ending every file
[*]
insert_final_newline = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,rb}]
charset = utf-8

# 2 space indentation
[*.{js,rb}]
indent_style = space
indent_size = 2
